@{
    ViewData["Title"] = local["Title"].Value;
}
@inject IViewLocalizer local
<div asp-validation-summary="ModelOnly"></div>
<form asp-antiforgery="true" method="post" enctype="multipart/form-data">
    <div>
        <label>@local["file"]</label>
        @Html.TextBox("image","",new {type="file"})
    </div>
    <button type="submit">
        @local["submit"]
    </button>
</form>
